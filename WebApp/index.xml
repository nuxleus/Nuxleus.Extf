<?xml-stylesheet type="text/xsl" href="/transform/build-output.xsl"?>
<my:session xmlns:html="http://www.w3.org/1999/xhtml"
  xmlns:bVend="http://xameleon.org/service/browservendors"
  xmlns:my="http://xameleon.org/service/session"
  xmlns:page="http://xameleon.org/service/page/output">
  <my:page>
    <page:config>
      <bVend:bVendors>
        <bVend:vBrowser vendor="Microsoft">
          <page:head.include fileType="css" href="/static/css/ie.css"/>
          <page:head.include fileType="css" href="/static/css/ie-webClipControl.css"/>
          <page:head.include fileType="javascript" href="/static/js/ie.js"/>
          <page:head.include fileType="javascript" href="/static/js/ie-liveclip.js"/>
        </bVend:vBrowser>
        <bVend:vBrowser vendor="Transformiix">
          <page:head.include fileType="css" href="/static/css/mozilla.css"/>
          <page:head.include fileType="css" href="/static/css/mozilla-webClipControl.css"/>
          <page:head.include fileType="javascript" href="/static/js/mozilla.js"/>
          <page:head.include fileType="javascript" href="/static/js/mozilla-liveclip.js"/>
        </bVend:vBrowser>
        <bVend:vBrowser vendor="libxslt">
          <page:head.include fileType="css" href="/static/css/safari.css"/>
          <page:head.include fileType="css" href="/static/css/safari-webClipControl.css"/>
          <page:head.include fileType="javascript" href="/static/js/safari.js"/>
          <page:head.include fileType="javascript" href="/static/js/safari-liveclip.js"/>
        </bVend:vBrowser>
        <bVend:vBrowser vendor="Opera">
          <page:head.include fileType="css" href="/static/css/opera.css"/>
          <page:head.include fileType="css" href="/static/css/opera-webClipControl.css"/>
          <page:head.include fileType="javascript" href="/static/js/opera.js"/>
          <page:head.include fileType="javascript" href="/static/js/opera-liveclip.js"/>
        </bVend:vBrowser>
      </bVend:bVendors>
    </page:config>
    <page:output>
      <page:head>
        <page:title>Title of Entry</page:title>
        <page:head.include fileType="css" href="/static/css/base.css"/>
        <page:head.include fileType="javascript" href="/static/microformat/hCal.js"/>
        <page:head.include fileType="javascript" href="/static/microformat/hCard.js"/>
        <page:head.include fileType="javascript" href="/static/js/script.js"/>
      </page:head>
      <page:body>
        <page:content src="/service/pub/"/>
      </page:body>
    </page:output>
  </my:page>
</my:session>
