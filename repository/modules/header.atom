	<link href="header.atom" rel="self" title="Header" type="application/atom+xml"/>
	<content type="application/xml">
		<module id="header" xmlns="http://x2x2x.org/atomicxml/system">
			<list>
				<module id="fixed-header">
					<list>
						<module id="top-navigation">
							<list direction="LtoR">
								<item id="current-location" class="inline alignLeft">
									<list>
										<item class="selected">
											<author/>
										</item>
										<item>:</item>
										<item class="inline alignLeft">
											<list src="/service/pub/"/>
										</item>
									</list>
								</item>
								<item id="quick-nav">
									<list>

										<item class="date">
											<date format="[MNn] [D1o], [Y]"/>
										</item>

									</list>
								</item>
							</list>
						</module>
						<module id="sub-nav">
							<list direction="LtoR">
								<item class="inline alignLeft">
									<list>
										<item class="inline alignLeft">
											<list src="/service/pub/" select="app:workspace"/>
										</item>
									</list>
								</item>
							</list>
						</module>
					</list>
				</module>
			</list>
		</module>
	</content>
